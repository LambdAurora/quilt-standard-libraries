{
  "schemaVersion": 1,
  "id": "quilt_resource_loader",
  "name": "Quilt Resource Loader",
  "version": "${version}",
  "environment": "*",
  "license": "Apache-2.0",
  "icon": "assets/quilt_resource_loader/icon.png",
  "contact": {
    "homepage": "https://quiltmc.org",
    "issues": "https://github.com/QuiltMC/quilt-standard-libraries/issues",
    "sources": "https://github.com/QuiltMC/quilt-standard-libraries"
  },
  "authors": [
    "QuiltMC"
  ],
  "depends": {
    "fabricloader": ">=0.13.1",
    "minecraft": ">=1.18.2",
    "quilt_base": "*"
  },
  "description": "Asset and data resource loading.",
  "accessWidener": "quilt_resource_loader.accesswidener",
  "mixins": [
    "quilt_resource_loader.mixins.json"
  ],
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_3262": [
        "org/quiltmc/qsl/resource/loader/api/QuiltResourcePack"
      ]
    },
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": {
        "id": "qsl",
        "name": "Quilt Standard Libraries",
        "description": "A set of libraries to assist in making Quilt mods.",
        "icon": "assets/quilt_resource_loader/icon.png",
        "badges": [
          "library"
        ]
      }
    }
  }
}
